                    "ConfigureSampleApp": {
                        "services": {
                            "systemd": {
                                "registration": {
                                    "enabled": "true",
                                    "ensureRunning": "true"
                                }
                            }
                        }
                    }
                }
            },
            "DependsOn": [
                "PublicRoute"
            ]
        },
